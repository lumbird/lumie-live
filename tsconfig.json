{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    //"jsx": "react",  // Make sure this is set appropriately
    "strict": true,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "sourceMap": true,
    "forceConsistentCasingInFileNames": false
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "node_modules/**/*",
    "android/**/*",
    "build/**/*",
    "www/**/*"
  ]
}